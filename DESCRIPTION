Package: bodycompref
Type: Package
Title: Reference values for CT-based body composition
Version: 1.0.0
Authors@R: 
  person(given = "J. Peter",
         family = "Marquardt",
         role = c("aut", "cre"),
         email = "peter@kmarquardt.de",
